{
    "version": 2,
    "builds": [
       {
         "src": "index.js",
         "use": "@vercel/node"
       }
    ],
    "routes": [
       {
         "src": "/api/(.*)",
         "dest": "/index.js"
       }
    ],
    "env": {
       "MONGO_URL": "@mongo_url",
       "JWT_SECRET": "@jwt_secret"
    }
}
   